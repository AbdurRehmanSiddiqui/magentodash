<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="customer_account_navigation">
            <block class="Magento\Customer\Block\Account\SortLinkInterface" name="add_student_block">
                <arguments>
                    <argument name="label" xsi:type="string">Add Student</argument>
                    <argument name="path" xsi:type="string">pointeger_crud/post/form</argument>
                </arguments>
            </block>
            <block class="Magento\Customer\Block\Account\SortLinkInterface" name="view_student_data">
                <arguments>
                    <argument name="label" xsi:type="string">View Student Data</argument>
                    <argument name="path" xsi:type="string">pointeger_crud/student/load</argument>
                </arguments>
            </block>
        </referenceBlock>
    </body>
</page>


